{
  "name": "sphericalcontour",
  "version": "1.0.0",
  "description": "Standalone version of the Spherical contours notebook",
  "main": "index.js",
  "scripts": {
    "data": "node bin/download-data.js",
    "build": "rollup -c",
    "deploy": "now",
    "predeploy": "yarn build",
    "preinstall": "mkdir -p src/notebook && curl -o - 'https://api.observablehq.com/@fil/spherical-contours.tgz?v=3' | tar xzf - --directory src/notebook",
    "preserve": "yarn build",
    "postinstall": "yarn data && yarn build",
    "serve": "http-server public/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "download-data": "bin/download-data.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "aaa_notebook": "file:src/notebook",
    "@observablehq/runtime": "^4.4.1",
    "d3": "^5.11.0",
    "d3-geo-projection": "^2.7.0",
    "d3-geo-voronoi": "^1.5.0",
    "d3-require": "^1.2.3",
    "http-server": "^0.11.1",
    "now": "^16.1.2",
    "request": "^2.88.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  }
}
